{
  "name": "my-turborepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "test": "turbo run test",
    "prune": "turbo run prune",
    "format": "prettier --write \"**/*.{ts,md}\"",
    "lint": "eslint --fix \"**/*.ts\""
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.108",
    "@types/jest": "29.2.2",
    "@types/node": "18.11.9",
    "@typescript-eslint/eslint-plugin": "5.42.1",
    "@typescript-eslint/parser": "5.42.1",
    "eslint-config-custom": "*",
    "jest": "29.3.1",
    "prettier": "latest",
    "ts-jest": "29.0.3",
    "turbo": "latest",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "npm@8.11.0"
}
